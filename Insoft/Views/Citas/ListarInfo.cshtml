@using Insoft.Entities
@model Insoft.Models.CitaBuscarViewModel
@{
    ViewBag.Title = "Listar Info";
}
<h2>Mantenimiento de Vehículos</h2>
@using (Html.BeginForm("ListarInfo", "Citas", FormMethod.Post))
{
    @Html.AntiForgeryToken()    
<div class="form-horizontal">
    <h4>Lista de atenciones para este cliente con esta placa </h4>
    <hr />
    @Html.ValidationSummary(true)
        @{
            ViewBag.Title = "Lista de Citas para este cliente con esta placa";
        }    
        <h4>Y si vas a guardar NEXT</h4>  
        <div>  
            <table id="t01"> 
                <tr> 
               <h4>
                <thead>  
                    <th>Id</th>  
                    <th>Placa</th>  
                    <th>Fecha</th>  
                    <th>Estado</th>                  
                </thead>  
                </h4>
                </tr>
                @if (ViewBag.userdetails != null)
                {
                     @foreach (var item in ViewBag.userdetails)  
                     {
                    
                            <tr>  
                                    <td>  
                        @item.Id  
                        </td>  
                                    <td>  
                        @item.Placa  
                        </td>  
                                    <td>  
                        @item.Fecha  
                        </td>  
                                    <td>  
                        @item.Estado  
                        </td>        
                                </tr>    
                     }    
                }
            </table>  
        </div>   
    
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">      
             <input type="submit" name="BtnPrevious" value="Previous" class="btn btn-default" />
            <input type="submit" name="BtnNext" value="Next" class="btn btn-default" />
        </div>
    </div>  

</div>
} 